    <script>
      function erase(event) {
        event.target.remove();
      }
      function getName(button) {
        let name = document.getElementById("name");
        name.textContent = button.textContent;
      }
      function change() {
        let btn = document.getElementById("color");
        btn.style.backgroundColor = "purple";
      }
      function total() {
        let num1 = document.getElementById("num1").value;
        let num2 = document.getElementById("num2").value;
        let sum = parseInt(num1) + parseInt(num2);
        let p = document.getElementById("answer");
        p.textContent = "Sum: " + sum;
      }
      function text() {
        let inputBox = document.getElementById("inputBox").value;
        let result = document.getElementById("result");
        result.textContent = inputBox;
      }
      function parallel() {
        let inputBox = document.getElementById("inputBox").value;
        let result = document.getElementById("result");
        let list = document.createElement("h1");
        list.textContent = inputBox;
        result.append(list);
